		"test": "tslib test",
		"update": "ncu -u",
		"postupdate": "lerna exec -- ncu -u"
	},
	"prettier": "@osdevisnot/prettier",
	"devDependencies": {
		"@osdevisnot/prettier": "2.1.0",
